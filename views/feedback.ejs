<%-include("header")-%>
    <div class="Feedback">
        <div class="feed">Please Provide Your Valuable Feedback</div>
        <form action="/feedback" method="post">
        <h3>Enter Your Name:</h3>
        <input type="text" name="yourName" class="input" placeholder="Enter Your full Name" required>
        <h3>Enter Email Id:</h3>
        <input type="email" name="email" class="input" placeholder="Enter Your Email Id" required>
        <h3>Type Your Content Below:</h3>
        <textarea name="item" id="poem" class="input" cols="50" rows="10" placeholder="Enter a relevant content......"></textarea>
        <button type="submit" name="myContent" >Compose Your Content</button>
        </form>
    </div>
<%-include("footer")-%>