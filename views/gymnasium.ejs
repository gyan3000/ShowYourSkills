<%-include("header")-%>
    <div class="right "><h1 class="homeHeader">Gymnasium Club Welcomes You</h1>
        <div class="content">
            <h3>About Gymnasium Club</h3>
            <p>
                Ignite your gym-freek.
                Get motivation quotes and motivations for the Inter-IIT and make your college proud in the Tournament.
                Here we are introducing the Gymnasium club for the health and fitness club.
                Flex your skills and showcase them to youe peers.  
            </p>
            <%posts.forEach(function(post){%>
                <div class="post">
                    <div class="heading">
                        <div class="_name"><%=post.name%></div>
                        <div class="_time">#<%=post.time%></div>
                    </div>
                    <div class="image">
                        <img src="/gymUploads/<%=post.image%>" alt="." style: height="300 rem">
                    </div>
                    
                </div>
            <%})%>
        </div>
        <div class="_btn">
            <a href="/gymnasium/gymnasiumUpload" class="btn">Upload Your Content <i class="fa-solid fa-arrow-up-from-bracket"></i></a>
        </div>  
    </div>
<%-include("footer")-%>
